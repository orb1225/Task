alter table st_shopee_shop_coin_earn_milestone_fatdt0 drop partition(pt=${pt});
insert overwrite table st_shopee_shop_coin_earn_milestone_fatdt0 partition(pt=${pt})
select "${dt}" as insert_date,shopid,count(itemid)
from
(
   select shopid,itemid,count(sku_id)
   from
   st_shopee_sku_coinearn_milestone_fatdt0
   where pt=${pt}
   group by
   shopid,itemid
) a
group by
shopid;